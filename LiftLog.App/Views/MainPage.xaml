<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    x:Class="LiftLog.App.Views.MainPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:d="http://schemas.microsoft.com/dotnet/2021/maui/design"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:LiftLog.App"
    xmlns:webUi="clr-namespace:LiftLog.WebUi;assembly=LiftLog.WebUi"
    Title="LiftLog.App"
    mc:Ignorable="d">
    <ContentPage.Content>
       
        <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
            <BlazorWebView.RootComponents>
                <RootComponent Selector="#app" ComponentType="{x:Type webUi:WebApplication}" />
            </BlazorWebView.RootComponents>
        </BlazorWebView>
    </ContentPage.Content>
</ContentPage>
