@inherits Fluxor.Blazor.Web.Components.FluxorComponent
@page "/session"
@inject NavigationManager NavigationManager;

@if (WorkoutSessionState.Value.Day is null)
{
    NavigationManager.NavigateTo("/");
}
else
{
    <Session CycleRepcountForExercise=@CycleRepcountForExercise WorkoutSession=@(WorkoutSessionState.Value.Day) LastSetFinishedAt=WorkoutSessionState.Value.RestTimerStart ></Session>
}
